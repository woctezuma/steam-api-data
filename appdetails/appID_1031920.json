  "name": "\u0421\u0438\u043c\u0443\u043b\u044f\u0442\u043e\u0440 \u0421\u0438\u0434\u0435\u043d\u0438\u044f \u0443 \u041f\u043e\u0434\u044a\u0435\u0437\u0434\u0430",
  "steam_appid": 1031920,
